
<%blocks.each do |block| %>
<div class="panel panel-default" data-id="<%= block.id %>" style="display: none;">
  <div class="panel-heading">
    <h3 class="panel-title"><%= block.category.name%></h3>
  </div>
  <div class="panel-body">
     <% block.category.tasks.each do |task| %>
     <p><%= task.name %></p>
     <% end %>
  </div>
</div>
<% end %>
